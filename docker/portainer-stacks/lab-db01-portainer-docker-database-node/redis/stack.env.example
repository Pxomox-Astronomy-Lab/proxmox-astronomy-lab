# Redis Container
CONTAINER_NAME=lab-redis01-lab-db01
REDIS_VERSION=7.0

# Networking
REDIS_PORT=6379

# Storage
VOLUME_DATA=redis_data

# Redis Authentication (Traditional Variables)
REDIS_PASSWORD=changeme
